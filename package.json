{
    "name": "arctia",
    "version": "2.0.0",
    "publisher": "Amaru",
    "engines": {
        "vscode": "^1.66.0"
    },
    "license": "AGPL-3.0-only",
    "displayName": "Arctia",
    "description": "Arctia is a remote controller for Cider 2, the open-source Apple Music client.",
    "categories": [
        "Other"
    ],
    "keywords": [
        "arctia",
        "music",
        "remote",
        "controller",
        "cider"
    ],
    "icon": "images/logo.png",
    "preview": true,
    "main": "./extension.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/Amaru8/Arctia"
    },
    "contributes": {
        "commands": [
            {
                "command": "arctia.playpause",
                "title": "Arctia: Play/Pause"
            },
            {
                "command": "arctia.nextSong",
                "title": "Arctia: Next Song"
            },
            {
                "command": "arctia.previousSong",
                "title": "Arctia: Previous Song"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "arctia-sidebar-view",
                    "title": "Cider - Arctia",
                    "icon": "images/cider-icon.svg"
                }
            ]
        },
        "views": {
            "arctia-sidebar-view": [
                {
                    "type": "webview",
                    "id": "arctia-sidebar",
                    "name": "Cider - Arctia",
                    "icon": "images/cider-icon.svg",
                    "contextualTitle": "Cider - Arctia"
                }
            ]
        }
    },
    "activationEvents": [
        "onView:arctia-sidebar",
        "onCommand:arctia.playpause",
        "onCommand:arctia.nextSong",
        "onCommand:arctia.previousSong"
    ],
    "scripts": {
        "lint": "eslint .",
        "pretest": "yarn run lint"
    },
    "devDependencies": {
        "@vscode/test-electron": "^2.1.3",
        "eslint": "^8.11.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.2"
    },
    "extensionKind": [
        "ui"
    ],
    "dependencies": {
        "node-fetch": "^2.6.11"
    }
}
